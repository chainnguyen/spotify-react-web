{
  "name": "spotify-react-web",
  "private": false,
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Chain Nguyá»…n",
    "email": "ntrungtn@gmail.com",
    "url": "https://chainnguyen.github.io/chain-curriculum-vitae/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chainnguyen/spotify-react-web.git"
  },
  "scripts": {
    "dev": "vite --host",
    "build:development": "tsc && vite build --mode development",
    "build:staging": "tsc && vite build --mode staging",
    "build:production": "tsc && vite build",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "lint:eslint": "eslint --max-warnings 0  \"src/**/*.{ts,tsx}\" --fix",
    "lint:prettier": "prettier . --write  \"src/**/*.{js,json,tsx,html,md}\"",
    "format": "npm run lint:eslint && npm run lint:prettier",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1"
  },
  "devDependencies": {
    "@types/node": "~18.14.0",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitejs/plugin-legacy": "^1.5.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.2",
    "lodash-es": "^4.17.21",
    "postcss": "^8.4.21",
    "prettier": "2.8.1",
    "shipit-cli": "^5.3.0",
    "shipit-deploy": "^5.3.0",
    "shipit-slack": "^0.1.3",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  },
  "engines": {
    "node": ">= 14.18.0",
    "npm": ">= 6.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
